				"y": 469
			},
			"value_type": "Speaker",
			"speaker_id": ""
		},
		{
			"type": "dialog",
			"meta": {
				"x": 3702,
				"y": 469
			},
			"dialog_text": "Oh that's a little akward, ummm... Here, I have a spare here if you'd \nlike to read it..."
		},
		{
			"type": "setter",
			"meta": {
				"x": 1947,
				"y": 2201
			},
			"value_type": "Speaker",
			"speaker_id": ""
